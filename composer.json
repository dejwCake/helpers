{
    "name": "dejw-cake/helpers",
    "description": "DejwCake/Helpers plugin for CakePHP",
    "type": "cakephp-plugin",
    "keywords": [
        "helper",
        "slugable",
        "enable",
        "translate",
        "sortabe",
        "tree"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "DejwCake",
            "email": "github@dejw.sk",
            "role": "Author"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": ">=3.3.2 <4.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "DejwCake\\Helpers\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DejwCake\\Helpers\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
